{
  "kind": "collectionType",
  "collectionName": "ads",
  "info": {
    "name": "Ads",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "user": {
      "via": "ads",
      "plugin": "users-permissions",
      "model": "user"
    },
    "sport": {
      "model": "sports",
      "via": "ads"
    },
    "Date": {
      "type": "string"
    },
    "location": {
      "model": "locations",
      "via": "ads"
    },
    "image": {
      "type": "string"
    },
    "views": {
      "type": "integer",
      "default": 0
    },
    "requests": {
      "via": "ad",
      "collection": "requests"
    },
    "notifications": {
      "via": "ad",
      "collection": "notifications"
    }
  }
}
